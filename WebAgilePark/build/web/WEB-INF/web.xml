<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <welcome-file-list>
        <welcome-file>iniciosesion.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>Acceder</servlet-name>
        <servlet-class>controladores.Acceder</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registroV</servlet-name>
        <servlet-class>controladores.registroV</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registroUs</servlet-name>
        <servlet-class>controladores.registroUs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertCliente</servlet-name>
        <servlet-class>controladores.InsertCliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>consulPlaca</servlet-name>
        <servlet-class>modelos.consulPlaca</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConsultarPlaca</servlet-name>
        <servlet-class>controladores.ConsultarPlaca</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>consultasGenerales</servlet-name>
        <servlet-class>controladores.consultasGenerales</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Acceder</servlet-name>
        <url-pattern>/Acceder</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registroV</servlet-name>
        <url-pattern>/vehiculo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registroUs</servlet-name>
        <url-pattern>/usuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertCliente</servlet-name>
        <url-pattern>/RegisEm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>consulPlaca</servlet-name>
        <url-pattern>/placa_E</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConsultarPlaca</servlet-name>
        <url-pattern>/ConsultarPlaca</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>consultasGenerales</servlet-name>
        <url-pattern>/consultasGenerales</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
